        skinsec = "Cyberpunk"
    end
end)

beautySection:Toggle("开启皮肤美化", "EnableBeauty", false, function(state)
    if state then
        local inventory = item.inventory
        local items = inventory.items
        for _, item in ipairs(items) do
            if item.type == "Gun" then
                inventory.skinUpdate(item.name, skinsec)
            end
        end
    end
end)
